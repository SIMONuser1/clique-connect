<% business.also_clicked.each do |list| %>
  <li>
    <div class="media">
        <%= cl_image_tag list.photo, class: 'avatar avatar-square rounded mr-3' %>
      <div class="media-body">
        <a href="#">
          <span class="h6"><%= list.name %></span>
        </a>
        <span class="badge badge-secondary"><%= business.industries_match(list).first %></span>
        <div class="text-muted">
            <ul class="list-inline">
              <li class="list-inline-item">
                <small><i class="fas fa-hand-holding-heart"> </i> <%= business.match_percent_with(list) %>% Match</small>
              </li>
            </ul>
        </div>
      </div>
    </div>
  </li>
<% end %>
