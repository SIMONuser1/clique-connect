<% @business.users.each do |list| %>
  <li>
    <%= link_to list.linkedin_url, class: "media", target: "_blank" do %>
      <%= cl_image_tag list.avatar, class: 'avatar avatar-sm mr-3' %>
      <div class="media-body">
        <span class="h6 mb-0"><%= list.full_name %></span>
        <span class="text-muted"><%= list.location %></span>
      </div>
    <% end %>
  </li>
<% end %>
